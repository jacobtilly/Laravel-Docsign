{
    "name": "jacobtilly/laravel-docsign",
    "description": "A package to easily use Inleed Docsign in Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Jacob Tilly",
            "email": "dev@jacobtilly.com",
            "homepage": "http://jacobtilly.com"
        }
    ],
    "homepage": "https://github.com/jacobtilly/laravel-docsign",
    "keywords": ["Laravel", "LaravelDocsign"],
    "require": {
        "illuminate/support": "^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "orchestra/testbench-core": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "JacobTilly\\LaravelDocsign\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JacobTilly\\LaravelDocsign\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JacobTilly\\LaravelDocsign\\LaravelDocsignServiceProvider"
            ],
            "aliases": {
                "LaravelDocsign": "JacobTilly\\LaravelDocsign\\Facades\\LaravelDocsign"
            }
        }
    }
}
